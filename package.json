{
  "name": "yat_app",
  "version": "0.0.1",
  "description": "todo app",
  "main": "lib/index.js",
  "repository": "https://github.com/gabemeola/yat_app",
  "author": "Gabe M <gabemeola@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node lib/index.js",
    "predev": "yarn clean",
    "dev": "yarn build:server:watch",
    "prebuild": "yarn run clean",
    "build": "NODE_ENV=production concurrently -p '[{name}]' -n 'Client,Server' -c 'bgBlue.bold,bgMagenta.bold' 'yarn build:client' 'yarn build:server'",
    "build:client": "BUILD_TARGET=browser NODE_ENV=production webpack",
    "build:server": "BUILD_TARGET=server NODE_ENV=production webpack",
    "build:server:watch": "BUILD_TARGET=server webpack --watch",
    "clean": "rm -rf ./lib",
    "test": "jest",
    "lint": "yarn lint:js",
    "lint:js": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint:js --fix",
    "commit": "crank commit",
    "c": "crank commit"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "source-map-support": "^0.5.9"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.0",
    "@types/helmet": "^0.0.42",
    "@types/jest": "23.3.10",
    "@types/node": "10.12.12",
    "@types/react": "16.7.13",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@zelz/crank": "^0.1.2",
    "concurrently": "^4.1.0",
    "eslint": "^5.10.0",
    "eslint-plugin-react-hooks": "^0.0.0",
    "jest": "^23.6.0",
    "less": "^3.9.0",
    "react-hot-loader": "^4.3.12",
    "typescript": "3.2.2",
    "typescript-eslint-parser": "^21.0.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
